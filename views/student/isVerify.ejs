<section class="mt-5">
    <div class="container">
        <div class="row">
            <div class="col-md-3"></div>
            <div class="col-md-6">
                <a class="btn btn-primary" href="/student">Back to Home</a>
                <div class="card mt-2">
                    <div class="card-body">
                        <% if (isVerifyed == true) { %>
                            <img class="w-100" src="https://i.gifer.com/FqEh.gif" alt="">
                            <% } else { %>
                                <img class="w-100" src="https://media1.giphy.com/media/lQD1lvFH1wKXePWWlg/giphy.gif?cid=6c09b9523nbffamlcez2b7zr4m3x9r2uqxie4ny5pdnz8wch&rid=giphy.gif&ct=s" alt="">
                                <% } %>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<% if (isVerifyed == true) { %>
    <script>
        Swal.fire({
            position: 'top-center',
            icon: 'success',
            title: 'Your verification has been success',
            showConfirmButton: false,
            timer: 1500
        })
    </script>
    <% } else { %>

        <script>
            Swal.fire({
                position: 'top-center',
                icon: 'error',
                title: 'Your verification has been Taken!',
                showConfirmButton: false,
                timer: 1500
            })
        </script>

        <% } %>